        <parent link="h_camera_link" />
        <child link="h_camera_color_frame" />
    </joint>
    <link name="h_camera_color_frame" />
    <joint name="h_camera_color_optical_joint" type="fixed">
        <origin rpy="-1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0" />
        <parent link="h_camera_color_frame" />
        <child link="h_camera_color_optical_frame" />
    </joint>
    <link name="h_camera_color_optical_frame" />
